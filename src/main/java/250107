class Solution {
    public String solution(String my_string) {
      
        String vowels = "aeiou";
    
        StringBuilder answer = new StringBuilder();
     
        for (int i = 0; i < my_string.length(); i++) {
            char ch = my_string.charAt(i);
           
            if (vowels.indexOf(ch) == -1) {
                answer.append(ch);
            }
        }
        
    
        return answer.toString();
    }
}
