import java.util.Scanner;

public class b3052 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int [] ary=new int[10];
		int []ary2=new int[10];
		int count=0;
		
		for(int i=0; i<10; i++) {
			ary[i]=s.nextInt();	
		}
		
		for(int i=0; i<10; i++) {
			ary2[i]=ary[i]%42;
		}
		
		for(int i=0;i<10;i++) {
			if(ary2[0]!=ary2[i]) {
				count++;
				break;
			}
		}
	
		System.out.print(count);
		
	}

}
